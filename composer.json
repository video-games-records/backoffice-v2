{
    "name": "projet-normandie/symfony-skeleton",
    "description": "Symfony Skeleton Project",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.4",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-pdo": "*",
        "a2lix/translation-form-bundle": "^3.2.4",
        "api-platform/doctrine-orm": "^4.2.8",
        "api-platform/graphql": "^4.2.8",
        "api-platform/symfony": "^4.2.8",
        "aws/aws-sdk-php-symfony": ">=2.8",
        "beberlei/doctrineextensions": "^1.5",
        "damienharper/auditor-bundle": "^6.2.3",
        "doctrine/dbal": "^3.10.4",
        "doctrine/doctrine-bundle": "^2.18.1",
        "doctrine/doctrine-migrations-bundle": "^3.7",
        "doctrine/orm": "^3.5.8",
        "ezyang/htmlpurifier": "^4.19",
        "gesdinet/jwt-refresh-token-bundle": "^1.5",
        "google/apiclient": "^2.18.4",
        "kris-kuiper/igdbv4": "^1.1.5",
        "league/flysystem-aws-s3-v3": ">=3.30.1",
        "league/flysystem-bundle": "^3.6",
        "nelmio/cors-bundle": "^2.6",
        "phpdocumentor/reflection-docblock": "^5.6.5",
        "phpstan/phpdoc-parser": "^2.3",
        "sonata-project/admin-bundle": "^4.41",
        "sonata-project/doctrine-orm-admin-bundle": "^4.19",
        "sonata-project/translation-bundle": "^4.x-dev",
        "sonata-project/user-bundle": "^5.18",
        "stof/doctrine-extensions-bundle": "^1.14",
        "symfony/amazon-mailer": "7.4.*",
        "symfony/asset": "^7.4",
        "symfony/brevo-mailer": "^7.4.0",
        "symfony/console": "^7.4.0",
        "symfony/discord-notifier": "7.4.*",
        "symfony/doctrine-messenger": "^7.4.0",
        "symfony/dotenv": "^7.4.0",
        "symfony/event-dispatcher": "^7.4.0",
        "symfony/expression-language": "^7.4.0",
        "symfony/flex": "^2.10",
        "symfony/framework-bundle": "^7.4.0",
        "symfony/http-client": "^7.4.0",
        "symfony/mailer": "^7.4.0",
        "symfony/messenger": "^7.4.0",
        "symfony/monolog-bundle": "^3.11",
        "symfony/notifier": "7.3.*",
        "symfony/property-access": "^7.4.0",
        "symfony/property-info": "^7.4.0",
        "symfony/rate-limiter": "7.3.*",
        "symfony/runtime": "^7.4.0",
        "symfony/scheduler": "7.3.*",
        "symfony/security-bundle": "^7.4.0",
        "symfony/serializer": "^7.4.0",
        "symfony/twig-bundle": "^7.4.0",
        "symfony/validator": "^7.4.0",
        "symfony/yaml": "^7.4.0",
        "yokai/sonata-workflow": "^0.9.0",
        "zenstruck/messenger-monitor-bundle": "^0.5.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^8.4",
        "doctrine/doctrine-fixtures-bundle": "^4.3.1",
        "fakerphp/faker": "^1.24.1",
        "justinrainbow/json-schema": "^6.6.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-symfony": "^2.0.9",
        "squizlabs/php_codesniffer": "^3.13.5",
        "symfony/browser-kit": "7.3.*",
        "symfony/css-selector": "7.3.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/phpunit-bridge": "^7.4.0",
        "zenstruck/foundry": "^2.8.1"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "console": "@php bin/console",
        "make:db": [
            "@console doctrine:database:create --if-not-exists",
            "@console doctrine:schema:drop --force --full-database",
            "@console doctrine:schema:create",
            "@console doctrine:fixtures:load -n"
        ],
        "make:db:test": [
            "@console doctrine:database:create --if-not-exists --env=test",
            "@console doctrine:schema:drop --force --full-database --env=test",
            "@console doctrine:schema:create --env=test",
            "@console d:fixtures:load --env=test -n"
        ],
        "lint:phpstan": "@php vendor/bin/phpstan",
        "lint:phpcs": "@php vendor/bin/phpcs src/ tests/ -p",
        "lint:phpcs:fix": "@php vendor/bin/phpcbf src/ tests/ -p",
        "lint": [
            "@lint:phpcs",
            "@lint:phpstan"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.4.*"
        }
    }
}
